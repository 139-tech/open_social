<ul>
    {% for notification in notifications %}
        <li>{{ notification|striptags('<p><a><strong><small><em><div><table><tbody><tr><td>')|raw }}</li>
    {% endfor %}
</ul>

{{ notification_footer }}

<p>&nbsp;</p>

<p><small>{{ notification_settings|striptags('<a><strong>')|raw }}</small></p>
